/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

@layer base {
	.font-outline {
		-webkit-text-stroke: 1px #2e0600;
	}
	.font-outline-2 {
		-webkit-text-stroke: 2px #2e0600;
	}
	.font-outline-4 {
		-webkit-text-stroke: 4px #2e0600;
	}
}
